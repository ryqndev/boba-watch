  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && cp build/index.html build/404.html",
    "beta": "npm version patch --no-git-tag-version && npm run build && cp -R build/. ../beta-boba-watch/ && cd ../beta-boba-watch/ && echo \"beta.boba.watch\" > CNAME && git add . && git commit -m \"New Update\" && git pushh",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm version patch --no-git-tag-version && npm run build",
    "en": "js-yaml src/i18n/translations/en_translation.yml > public/locales/en/translation.json",
    "zhTW": "js-yaml src/i18n/translations/zh-TW_translation.yml > public/locales/zh-TW/translation.json",
    "translations": "npm run en && npm run zhTW",
    "YESIKNOWWHATIMDOING": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "homepage": "https://boba.watch/"
}
